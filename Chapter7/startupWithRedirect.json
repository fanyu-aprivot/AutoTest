[
  {
    "description": "重定向到百度",
    "request": {
      "uri": "/redirect"
    },
    "redirectTo": "http://www.baidu.com"
  },
  {
    "description": "重定向到一个自己的网页上",
    "request": {
      "uri": "/redirect/old",
      "method": "get"
    },
    "redirectTo": "/redirect/new"
  },
  { 
    "description": "这是被重定向到的请求",
    "request": {
      "uri": "/redirect/new",
      "method": "get"
    },
    "response": {
      "text": "重定向成功啦"
    }
  }
]