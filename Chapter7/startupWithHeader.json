[
  {
    "description": "模拟带有header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "Content-type": "application/json"
      },
      "json": {
        "name": "lily",
        "sex": "woman"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "lily": "success",
        "status": 0
      }
    }

  }
]